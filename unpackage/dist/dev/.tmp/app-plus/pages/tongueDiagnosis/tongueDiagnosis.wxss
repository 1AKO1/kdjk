view.content {
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  width: 100%;
  padding: 10px;
  background: #FFFFFF;
}
.camera {
  width: 30vh;
  height: calc(30vh + 30px);
  margin: calc(10vh) auto;
}
.camera .s-camera {
    width: calc(30vh - 4px);
    height: calc(30vh - 4px);
    border-radius: 15vh;
    border: #40A9FF 2px solid;
    background: url(data:image/png;base64,) no-repeat center center;
}
.camera .tip {
    line-height: 30px;
    font-size: 16px;
    color: #888;
    text-align: center;
    white-space: nowrap;
}
.tips {
  width: calc(100vw - 20px);
  position: absolute;
  bottom: 15vh;
}
.tips text {
    font-size: 14px;
    color: #B8BDBF;
}

