	display: flex;
	-webkit-box-orient:horizontal;
	-webkit-box-direction:normal;
	-webkit-flex-direction:row;
	    -ms-flex-direction:row;
	        flex-direction:row;
	padding: 10rpx;
	width: 750rpx;
}
.button{
	width: 90% ! important;
	margin-left: 0rpx;
	margin-right: 50rpx;
	background-color: white ! important;
	padding: 0;
}
.list image{
	width: 460rpx;
	margin-right: 17rpx;
}
